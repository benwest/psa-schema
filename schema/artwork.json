{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "psa/artwork.json",
    "description": "Artwork in PSA Collections",
    "type": "object",
    "properties": {
        "slug": {
            "$ref": "definitions/slug.json"
        },
        "title": {
            "type": "string"
        },
        "artist": {
            "type": "string"
        },
        "images": {
            "type": "array",
            "items": {
                "$ref": "definitions/image.json"
            }
        },
        "collectionNumber": {
            "type": "string"
        },
        "date": {
            "type": "string"
        },
        "medium": {
            "type": "string"
        },
        "details": {
            "$ref": "definitions/details.json"
        },
        "blocks": {
            "$ref": "definitions/blocks.json"
        }
    },
    "required": [
        "slug",
        "title",
        "artist",
        "images",
        "collectionNumber",
        "date",
        "medium",
        "details",
        "blocks"
    ],
    "additionalProperties": false
}