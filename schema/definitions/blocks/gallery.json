{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "psa/definitions/blocks/gallery.json",
    "type": "object",
    "required": [ "files" ],
    "properties": {
        "type": { "const": "gallery" },
        "title": { "type": "string" },
