{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "psa/definitions/blocks/embed.json",
    "type": "object",
    "required": [
        "url"
    ],
    "additionalProperties": false,
    "properties": {
        "type": {
            "const": "embed"
        },
        "title": {
            "type": "string"
        },
        "url": {
            "type": "string",
            "format": "uri"
        },
        "ratio": {
            "type": "number"
        }
    }
}