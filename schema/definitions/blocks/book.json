{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "psa/definitions/blocks/carousel.json",
    "type": "object",
    "required": [
        "pages"
    ],
    "properties": {
        "type": {
            "const": "book"
        },
        "title": {
            "type": "string"
        },
        "pages": {
            "type": "array",
            "minItems": 1,
            "items": {
                "$ref": "../image.json"
            }
        }
    }
}