{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "psa/collection-item.json",
    "description": "Artwork in PSA Collections",
    "type": "object",
    "properties": {
        "slug": { "$ref": "definitions/slug.json" },
        "title": { "type": "string" },
        "artist": { "type": "string" },
        "collectionNumber": { "type": "string" },
        "date": { "type": "string" },
        "medium": { "type": "string" },
        "details": { "$ref": "definitions/details.json" },
        "blocks": { "$ref": "definitions/blocks.json" }
    },
    "required": [ "slug", "title", "collectionNumber", "artist", "date", "medium", "details", "blocks" ],
    "additionalProperties": false
}